{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/speaking",
      "dest": "/public/speaking.html"
    },
    {
      "src": "/writing",
      "dest": "/public/writing.html"
    },
    {
      "src": "/reading",
      "dest": "/public/reading.html"
    },
    {
      "src": "/progress",
      "dest": "/public/progress.html"
    },
    {
      "src": "/(css|js)/(.*)",
      "dest": "/public/$1/$2"
    }
  ]
}
